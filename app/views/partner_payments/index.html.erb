<p id="notice"><%= notice %></p>

<h1>Partner Payments</h1>

<table>
  <thead>
    <tr>
      <th>Payment type</th>
      <th>Date</th>
      <th>Amount</th>
      <th>Payment method</th>
      <th>Payment desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partner_payments.each do |partner_payment| %>
      <tr>
        <td><%= partner_payment.payment_type %></td>
        <td><%= partner_payment.date %></td>
        <td><%= partner_payment.amount %></td>
        <td><%= partner_payment.payment_method %></td>
        <td><%= partner_payment.payment_desc %></td>
        <td><%= link_to 'Show', partner_payment %></td>
        <td><%= link_to 'Edit', edit_partner_payment_path(partner_payment) %></td>
        <td><%= link_to 'Destroy', partner_payment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner Payment', new_partner_payment_path %>
