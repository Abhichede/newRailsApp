
$("form#investment_return_form").on('ajax:before', function (event, data, status) {
    console.log("Before");
});
$("form#investment_return_form").on('ajax:after', function (event, data, status) {
    console.log("After");
});
$("form#investment_return_form").on('ajax:success', function (event, data, status) {
    console.log("success");

    $(this).trigger('reset');
    $("#newInvestmentReturnModal").modal('hide');
    off();
    $.notify(
        "Investment returns was successfully added, see details for more information",
        {style: 'bootstrap', className: 'success'}
    );
    $("#investorTableBody").reload(true);


});
$("form#investment_return_form").on('ajax:error', function (event, data, status) {
    console.log("error");
    $.notify(
        "Something went wrong while saving investment return",
        {style: 'bootstrap', className: 'error'}
    );
});