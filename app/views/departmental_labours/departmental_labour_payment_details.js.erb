$(".dept_lab_pay_det-<%= @dept_lab.id %>").on('ajax:before', function () {
    show_spinner();
    console.log("Ajax Before ")
});

$(".dept_lab_pay_det-<%= @dept_lab.id %>").on('ajax:after', function () {
    hide_spinner();
    console.log("Ajax after ")
});

$(".dept_lab_pay_det-<%= @dept_lab.id %>").on('ajax:success', function () {
  <%  @dept_lab_outgoing_payment = OutgoingPayment.where(:payment_for => 'DEPARTMENTAL_LABOURS-'+@dept_lab.id.to_s,
                                                                        :site_id => @dept_lab.site_id) %>


    <%  @dept_lab_outgoing_payment.each do | details|  %>
        console.log(<%= details.paid_by.to_s %>);
    <% end %>
});
$(".dept_lab_pay_det-<%= @dept_lab.id %>").on('ajax:error', function () {
    show_spinner();
    console.log("Ajax Error ")
});